
<clickhouse>
  <logger><level>warning</level></logger>
  <query_log><flush_interval_milliseconds>700</flush_interval_milliseconds></query_log>
  <listen_host>::</listen_host>
  <prometheus>
    <endpoint>/metrics</endpoint>
    <port>9363</port>
    <metrics>true</metrics>
    <events>true</events>
    <asynchronous_metrics>true</asynchronous_metrics>
  </prometheus>
</clickhouse>
